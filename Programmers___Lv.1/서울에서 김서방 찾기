


// 풀이시간 00:04:11 & 제출점수 000점



class Solution {

    public String solution(String[] seoul) {
    
        String answer = "";
        
        for(int i=0; i<seoul.length; i++) {
			if(seoul[i].equals("Kim")) {
                Integer.toString(i);
				answer = "김서방은 " + i + "에 있다";
			}
		}
        
        return answer;
        
    }
}