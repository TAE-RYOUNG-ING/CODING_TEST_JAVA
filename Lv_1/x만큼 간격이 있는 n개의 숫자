


// 풀이시간 01:03:25 & 제출점수 100점



class Solution {
	
    public long[] solution(int x, int n) {
        
        long[] answer = new long[n];
        
        for(int i=0; i < n; i++){
            answer[i] = x * ((long)i+1);  // long타입으로 변환
        }
        
        return answer;
        
    }
}