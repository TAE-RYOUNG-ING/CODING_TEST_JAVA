


// 풀이시간 00:25:38 & 제출점수 100점



class Solution {
	public String solution(String phone_number) {
	    
	    String star = phone_number.substring(0, phone_number.length()-4).replaceAll("[0-9]", "*");
	    String last = phone_number.substring(phone_number.length()-4, phone_number.length());
	    
	    return star + last;
	}
}